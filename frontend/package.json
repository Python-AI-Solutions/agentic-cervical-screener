{
  "name": "agentic-cervical-screener",
  "version": "1.0.0",
  "description": "A web-based cervical screening tool that uses AI to classify and detect abnormal cells in cervical cytology images. The application combines a FastAPI backend with a modern web frontend for interactive image analysis.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "playwright test --reporter=line",
    "test:e2e:debug": "playwright test --debug",
    "test:vlm": "npm run vlm:docs && npm run vlm:viewer",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:vlm",
    "docs:test": "vitest run ../docs/__tests__/project_overview.index.test.ts",
    "docs:e2e": "cross-env PLAYWRIGHT_ARTIFACT_ROOT=./playwright-artifacts playwright test",
    "vlm:docs": "cross-env DOCS_VLM_SUITE=docs-overview tsx scripts/docs-overview-vlm.ts",
    "vlm:viewer": "cross-env DOCS_VLM_SUITE=viewer pixi run python scripts/vlm_viewer_audit.py",
    "docs:vlm-review": "npm run test:vlm",
    "docs:metrics": "tsx ../scripts/docs/onboarding-metrics.ts && tsx ../scripts/docs/check-doc-freshness.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Python-AI-Solutions/agentic-cervical-screener.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Python-AI-Solutions/agentic-cervical-screener/issues"
  },
  "homepage": "https://github.com/Python-AI-Solutions/agentic-cervical-screener#readme",
  "dependencies": {
    "@niivue/niivue": "^0.64.0",
    "marked": "^17.0.0",
    "tw-elements": "^2.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.2",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "execa": "^9.6.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^27.0.1",
    "papaparse": "^5.5.3",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vite": "^7.1.12",
    "vitest": "^4.0.4",
    "yaml": "^2.8.1"
  }
}
