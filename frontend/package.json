{
  "name": "agentic-cervical-screener",
  "version": "1.0.0",
  "description": "A web-based cervical screening tool that uses AI to classify and detect abnormal cells in cervical cytology images. The application combines a FastAPI backend with a modern web frontend for interactive image analysis.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": " Serving HTML report at http://localhost:9323. Press Ctrl+C to quit.",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm test && npm run test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Python-AI-Solutions/agentic-cervical-screener.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Python-AI-Solutions/agentic-cervical-screener/issues"
  },
  "homepage": "https://github.com/Python-AI-Solutions/agentic-cervical-screener#readme",
  "dependencies": {
    "@niivue/niivue": "^0.64.0",
    "tw-elements": "^2.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.2",
    "autoprefixer": "^10.4.22",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.4"
  }
}
